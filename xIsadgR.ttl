# ========== datePointType ========== #
###  https://github.com/KOST-CECO/ontologies/blob/master/xisadgr#datePointType
xisadgr:datePointType a owl:DatatypeProperty ;
  rdfs:label "datePointType" ;
  rdfs:comment "CC (Century); YYYY-MM-DDThh:mm; YYYY-MM-DDThh:mm:ss; YYYY; YYYY-MM; YYYY-MM-DD; unknown" ;
  owl:oneOf ( xsd:dateTime xsd:gYear xsd:gYearMonth xsd:date "unknown" xisadgr:centuryType xisadgr:dateTimeReducedType ) ;
.
  ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadgr#centuryType
  xisadgr:centuryType a owl:DatatypeProperty ;
    rdfs:label "centuryType" ;
    rdfs:comment "CC (Century)" ;
    owl:withRestrictions ( [ xsd:pattern "^[0-9]{2}$"^^xsd:string ] ) ;
  .
  ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadgr#dateTimeReducedType
  xisadgr:dateTimeReducedType a owl:DatatypeProperty ;
    rdfs:label "dateTimeReducedType" ;
    rdfs:comment "YYYY-MM-DDThh:mm" ;
    owl:withRestrictions ( [ xsd:pattern "^((000[1-9])|(00[1-9][0-9])|(0[1-9][0-9]{2})|([1-9][0-9]{3}))-((0[1-9])|(1[012]))-((0[1-9])|([12][0-9])|(3[01]))T(([01][0-9])|(2[0-3]))(:[0-5][0-9])$"^^xsd:string ] ) ;
  .

